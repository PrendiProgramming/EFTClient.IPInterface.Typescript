{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es6",
    "experimentalDecorators": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "esModuleInterop": true,
  },
  "include": [
    "./ts/**/*"
  ],
  "compileOnSave": true
}



